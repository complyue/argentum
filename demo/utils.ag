class Random {
    seed = 123456789;
    get(int from, int to) int {
        seed := seed * 5 + 12345;
        r = seed & 0x7fff_ffff_ffff_ffff >> 3;
        r % (to - from) + from
    }
}

fn forRangeWhile(int from, int to, (int)bool body) { // TODO replace bool with ?T after introduction of generics
    loop !(
        from < to && body(from) ?
            from += 1
    )
}

fn forRange(int from, int to, (int)void body) {
    loop !(from < to ? {
        body(from);
        from += 1
    })
}
